    "version": "0.2.0",
    "configurations": [
        {
            "name": "GofFabricWebApi[tsctsc6]",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/GofFabricWebApi/bin/Debug/net10.0/GofFabricWebApi.dll",
            "args": [],
            "cwd": "${workspaceFolder}/GofFabricWebApi",
            "stopAtEntry": false,
            // 禁用自动打开浏览器：移除 serverReadyAction
            "launchBrowser": { "enabled": false },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "tsctsc6"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "GofFabricWebApi[Jed]",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/GofFabricWebApi/bin/Debug/net10.0/GofFabricWebApi.dll",
            "args": [],
            "cwd": "${workspaceFolder}/GofFabricWebApi",
            "stopAtEntry": false,
            "launchBrowser": { "enabled": false },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Jed"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}